    <key>NSPrivacyExportCompliance</key>
    <dict>
        <key>usesEncryption</key>
        <true/>
        <key>encryptionInfo</key>
        <dict>
            <key>standardEncryption</key>
            <true/>
            <key>customEncryption</key>
            <false/>
            <key>encryptionTypes</key>
            <array>
                <string>HTTPS/TLS for network communication</string>
                <string>iOS Keychain for credential storage</string>
                <string>SwiftData encryption for local data</string>
                <string>iOS standard cryptographic libraries</string>
            </array>
            <key>exemptAlgorithms</key>
