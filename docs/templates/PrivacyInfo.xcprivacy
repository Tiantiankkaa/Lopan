        <key>version</key>
        <string>1.0.0</string>
        <key>privacyVersion</key>
        <string>1.0</string>
        <key>lastUpdated</key>
        <string>2025-09-26</string>
        <key>dataRetentionPolicy</key>
        <string>Production data retained according to business requirements. Personal preferences retained until app deletion.</string>
        <key>dataProcessingLawfulBasis</key>
        <array>
            <string>Legitimate Interest - Business Operations</string>
            <string>Consent - Optional Analytics</string>
            <string>Legal Obligation - Audit Requirements</string>
        </array>
        <key>userRights</key>
        <array>
            <string>Right to Access - Export functionality available</string>
            <string>Right to Rectification - Edit profile and preferences</string>
            <string>Right to Erasure - Account deletion available</string>
            <string>Data Portability - Export in standard formats</string>
        </array>
        <key>contactInfo</key>
        <dict>
            <key>privacyOfficer</key>
            <string>privacy@lopan.com</string>
            <key>supportEmail</key>
            <string>support@lopan.com</string>
            <key>dataProtectionOfficer</key>
            <string>dpo@lopan.com</string>
        </dict>
        <key>phase4Integration</key>
        <dict>
            <key>performanceMonitoring</key>
            <string>LopanPerformanceProfiler collects anonymous performance metrics</string>
            <key>memoryManagement</key>
            <string>LopanMemoryManager optimizes app performance without personal data collection</string>
            <key>productionMonitoring</key>
