        <!-- Example for analytics SDK:
        <dict>
            <key>NSPrivacyAccessedAPIType</key>
            <string>ExampleAnalyticsSDK</string>
            <key>NSPrivacyAccessedAPITypeReasons</key>
            <array>
                <string>Analytics and Performance Monitoring</string>
            </array>
        </dict>
        -->
    </array>

