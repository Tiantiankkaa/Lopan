        <key>primaryUseCase</key>
        <string>Production workflow management, inventory tracking, quality control</string>
        <key>dataTypes</key>
        <array>
            <string>Production schedules and batch information</string>
            <string>Inventory levels and movements</string>
            <string>Quality control metrics</string>
            <string>Customer order information</string>
            <string>Equipment and machine status</string>
        </array>
        <key>roles</key>
        <array>
            <string>Administrator - Full system access and configuration</string>
            <string>Salesperson - Customer and order management</string>
            <string>WarehouseKeeper - Inventory and stock management</string>
            <string>WorkshopManager - Production and machine management</string>
        </array>
        <key>complianceStandards</key>
        <array>
            <string>ISO 27001 - Information Security Management</string>
            <string>GDPR - European Data Protection Regulation</string>
            <string>CCPA - California Consumer Privacy Act</string>
            <string>SOC 2 Type II - Security and Availability</string>
        </array>
    </dict>
</dict>
